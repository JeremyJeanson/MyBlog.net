@model MyBlog.Models.PostsFilter
@Html.Partial("_PostsWithoutContent", Model.Items)
<script>
@if (Model.HaveMoreResults)
{
    <text>$("#btn-getmore").data("args", "@Model.Id?page=@Model.NextPage");</text>
}
else
{
    <text>$("#btn-getmore").hide();</text>
}
    $("#pagination").html('@Html.Raw(Html.Pagination(Model.Action, Model.Available, Model.Page, Settings.Current.PostQuantityPerSearch, new { Id = Model.Id }))');
</script>