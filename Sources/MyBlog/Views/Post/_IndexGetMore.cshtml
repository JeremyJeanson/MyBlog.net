@model MyBlog.Models.Posts
@foreach (var item in Model.Items)
{
    @Html.Partial("_PostWithSummary", item)
}
<script>
@if (Model.HaveMoreResults)
{
    <text>$("#btn-getmore").data("args","?page=@Model.NextPage");</text>
}
else
{
    <text>$("#btn-getmore").hide();</text>
}
    $("#pagination").html('@Html.Raw(Html.Pagination("Index", Model.Available, Model.Page, Settings.Current.PostQuantityPerPage))');
</script>
